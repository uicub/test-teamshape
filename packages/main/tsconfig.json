{
  "extends": "../tsconfig.react.json",
  "compilerOptions": {
    "composite": true,
    "noFallthroughCasesInSwitch": true,
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  },
  "include": [
    "src",
    "src/*",
    "src/**/*",
    "config-overrides.js",
    "src/extarnal"
  ]
}
